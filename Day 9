class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int num = 0, prev = 0;
        int res = 0;
        for(int i = 0; i < nums.size(); i++)
        {
            num = nums[i];
            
            if(target > prev && target <= num)
                res = i;
            
            else if(target > num)
                res = i+1;
            
            prev = nums[i];
            
        }
        return res;
    }
};
